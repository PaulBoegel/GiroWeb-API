{
  "env": {
    "browser": true,
    "es2021": true,
    "mocha": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-unused-vars": "warn",
    "no-param-reassign": 0
  }
}
